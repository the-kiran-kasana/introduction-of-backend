// Engine Class
class Engine {
  start() {
    console.log("Engine started");
  }
}

// Car Class (has Engine inside â†’ Tight Coupling)
class Car {
  constructor() {
    this.engine = new Engine(); // tightly coupled
  }

  drive() {
    this.engine.start();
    console.log("Car is driving");
  }
}

// Usage
const myCar = new Car();
myCar.drive();
